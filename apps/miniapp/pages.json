{
  "pages": [
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "河金小助手",
        "navigationStyle": "custom",
        "enablePullDownRefresh": false,
        "navigationBarTextStyle": "@navTextStyle",
        "navigationBarBackgroundColor": "@navBgColor",
        "backgroundColor": "@bgColor"
      }
    },
    {
      "path": "pages/profile/profile",
      "style": {
        "navigationBarTitleText": "个人信息",
        "navigationStyle": "custom",
        "navigationBarTextStyle": "@navTextStyle",
        "navigationBarBackgroundColor": "@navBgColor",
        "backgroundColor": "@bgColor"
      }
    }
  ],
  "subPackages": [
    {
      "root": "subpkg",
      "pages": [
        {
          "path": "course/course",
          "style": {
            "navigationBarTitleText": "课程安排",
            "navigationStyle": "custom",
            "disableScroll": true,
            "navigationBarTextStyle": "@navTextStyle",
            "navigationBarBackgroundColor": "@navBgColor",
            "backgroundColor": "@bgColor"
          }
        },
        {
          "path": "score/score",
          "style": {
            "navigationBarTitleText": "成绩查询",
            "navigationStyle": "custom",
            "navigationBarTextStyle": "@navTextStyle",
            "navigationBarBackgroundColor": "@navBgColor",
            "backgroundColor": "@bgColor"
          }
        },
        {
          "path": "exam/exam",
          "style": {
            "navigationBarTitleText": "考试安排",
            "navigationStyle": "custom",
            "navigationBarTextStyle": "@navTextStyle",
            "navigationBarBackgroundColor": "@navBgColor",
            "backgroundColor": "@bgColor"
          }
        },
        {
          "path": "elective/elective",
          "style": {
            "navigationBarTitleText": "选课信息",
            "navigationStyle": "custom",
            "navigationBarTextStyle": "@navTextStyle",
            "navigationBarBackgroundColor": "@navBgColor",
            "backgroundColor": "@bgColor"
          }
        },
        {
          "path": "plan/plan",
          "style": {
            "navigationBarTitleText": "执行计划",
            "navigationStyle": "custom",
            "navigationBarTextStyle": "@navTextStyle",
            "navigationBarBackgroundColor": "@navBgColor",
            "backgroundColor": "@bgColor"
          }
        },
        {
          "path": "dekt/dekt",
          "style": {
            "navigationBarTitleText": "第二课堂",
            "navigationStyle": "custom",
            "navigationBarTextStyle": "@navTextStyle",
            "navigationBarBackgroundColor": "@navBgColor",
            "backgroundColor": "@bgColor"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "@navTextStyle",
    "navigationBarTitleText": "河金小助手",
    "navigationBarBackgroundColor": "@navBgColor",
    "backgroundColor": "@bgColor",
    "backgroundColorTop": "@bgColor",
    "backgroundColorBottom": "@bgColor",
    "enablePullDownRefresh": false
  },
  "darkmode": true,
  "themeLocation": "theme.json",
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@/uni_modules/uni-$1/components/uni-$1/uni-$1.vue",
      "^Custom(.*)": "@/components/Custom$1/Custom$1.vue"
    }
  },
  "uniIdRouter": {}
}